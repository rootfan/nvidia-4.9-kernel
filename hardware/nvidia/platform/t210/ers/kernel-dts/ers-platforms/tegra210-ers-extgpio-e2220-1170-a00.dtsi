/ {
	i2c@7000c000 {
		pca9546@71 {
			compatible = "nxp,pca9546";
			reg = <0x71>;
			#address-cells = <1>;
			#size-cells = <0>;

			pca9546_i2c0: i2c@0 {
				reg = <0>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
			};
			pca9546_i2c1: i2c@1 {
				reg = <1>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
			};
			pca9546_i2c2: i2c@2 {
				reg = <2>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
			};
			pca9546_i2c3: i2c@3 {
				reg = <3>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;

				pca9539_74: gpio@74 {
					compatible = "nxp,pca9539";
					gpio-controller;
					#gpio-cells = <2>;
					reg = <0x74>;
					vcc-supply = <&battery_reg>;
				};

				pca9539_75: gpio@75 {
					compatible = "nxp,pca9539";
					gpio-controller;
					#gpio-cells = <2>;
					reg = <0x75>;
					vcc-supply = <&battery_reg>;
				};

				pca9539_76: gpio@76 {
					compatible = "nxp,pca9539";
					gpio-controller;
					#gpio-cells = <2>;
					reg = <0x76>;
					vcc-supply = <&battery_reg>;
				};
			};
		};
	};
};
