#include "tegra210-ers-e2220-1170-a00-00-common.dts"
#include "ers-platforms/tegra210-ers-1199-emc.dtsi"

/ {
	chosen {
		nvidia,tegra-shield-sku;
        };

	sdhci@700b0200 {
		nvidia,is-ddr-tap-delay;
		nvidia,ddr-tap-delay = <0>;
		dqs-trim-delay = <40>;
		dqs-trim-delay-hs533 = <24>;
		max-clk-limit = <266000000>;
		bus-width = <8>;
		uhs-mask = <0x60>;
		built-in;
		vddio_sdmmc-supply = <&max77620_sd3>;
		vddio_sd_slot-supply = <&vdd_3v3>;
		nvidia,dll-calib-needed;
		nvidia,is-emmc;
	};
	i2c@7000c000 {
		pca9546@71 {
			i2c@3 {
				gpio@76 {
					pcie-mux-output-low {
						gpio-hog;
						output-low;
						gpios = <12 0 14 0 15 0>;
						line-name = "pcie-mux-output-low";
					};

					pcie-mux-output-high {
						gpio-hog;
						output-high;
						gpios = <5 0 6 0 11 0 13 0>;
						line-name = "pcie-mux-output-high";
					};
				};
			};
		};
	};
};
