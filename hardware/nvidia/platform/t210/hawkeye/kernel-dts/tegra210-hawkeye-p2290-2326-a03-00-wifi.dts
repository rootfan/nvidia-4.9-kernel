#include "tegra210-hawkeye-p2290-3326-a03-00-base.dts"

/ {
	nvidia,dtsfilename = __FILE__;
	nvidia,boardids = "2290:3326:A03";
	nvidia,proc-boardid = "2290:3326:A03";

	i2c@7000c400 {
		tps61280@75 {
			status = "disabled";
		};
	};

	gpio@6000d000 {
		modem-regulator {
			gpio-hog;
			output-low;
			gpios = <TEGRA_GPIO(E, 0) 0
				   TEGRA_GPIO(E, 5) 0
				   TEGRA_GPIO(E, 1) 0>;
			line-name = "modem-regulator";
		};
	};
};
