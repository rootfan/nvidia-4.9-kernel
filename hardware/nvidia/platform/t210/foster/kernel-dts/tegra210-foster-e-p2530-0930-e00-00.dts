#include "tegra210-foster-e-p2530-common.dtsi"
#include <t210-common-platforms/tegra210-otg-gpio-detection-extcon.dtsi>
#include "foster-platforms/tegra210-loki-e-emc-a00.dtsi"
#include "foster-platforms/tegra210-foster-powermon-p2530-0930-e00.dtsi"

/ {
	model = "foster_e";
	compatible = "nvidia,foster-e", "nvidia,tegra210";
	nvidia,dtsfilename = __FILE__;
	nvidia,fastboot-usb-pid = <0xb421>;

	chosen {
		bootargs = "androidboot.secureos=tlk earlycon=uart8250,mmio32,0x70006000";
		stdout-path = &uarta;
	};

	firmware {
		android {
			compatible = "android,firmware";
			hardware = "foster_e";
		};
	};

	rollback-protection {
		status = "okay";
	};

	soctherm@0x700E2000 {
		throttle@oc3 {
			status = "disabled";
		};
	};
};
