/dts-v1/;

/memreserve/ 0x80000000 0x00020000;

#include <tegra210b01-soc/tegra210b01-soc-common-platform.dtsi>
#include "fpga-platforms/tegra210-grenada-fixed.dtsi"
#include <t210-common-platforms/tegra210-platform-common-padvoltage-default.dtsi>
#include <t210-common-platforms/tegra210-ers-power-dvfs-e2174-1101-a00.dtsi>
#include <dt-bindings/pinctrl/pinctrl-tegra.h>
