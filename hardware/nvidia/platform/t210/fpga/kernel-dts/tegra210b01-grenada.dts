		compatible = "nvidia,tegra210-asim";
		reg = < 0x0 0x538f0000 0x0 0x1000 >;
	};

	ethernet@0x4e000000 {
		compatible = "smsc,lan9115";
		reg = < 0x0 0x4e000000 0x0 0x00010000 >;
		interrupts = < 0  48 0x4 >;
		reg-io-width = <4>;
		smsc,irq-active-high;
		smsc,irq-push-pull;
	};

	pwm_regulators {
		gpu_ovr_reg: pwm-regulator@1 {
			/delete-property/ enable-gpio;
		};
	};
};
