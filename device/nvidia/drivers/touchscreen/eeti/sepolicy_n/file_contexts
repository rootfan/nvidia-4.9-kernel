# touch raw/log data
/data/eeti(/.*)?	u:object_r:eeti_touch_data_file:s0

# touch service
/odm/bin/eGalaxTPSrv		u:object_r:eeti_exec:s0

# touch FW
/odm/bin/exc80.bin		u:object_r:eeti_touch_fw_file:s0

# touch FW configuration file
/odm/bin/SensorTestDefault.ini		u:object_r:eeti_touch_fw_file:s0

# touch calibration sysfs
/sys/devices/virtual/misc/touch/calibration	u:object_r:sysfs_eeti_touch_calib:s0

